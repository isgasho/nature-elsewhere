{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://natureelsewhere.com/schemas/images/ImageRect.schema.json",
  "type": "object",
  "title": "ImageRect",
  "deepDefaults": true,
  "properties": {
    "bounds": {
      "$ref": "/schemas/math/Rect.schema.json",
      "default": {"x": 0, "y": 0, "w": 0, "h": 0}
    },
    "origin": {
      "$ref": "/schemas/math/XY.schema.json",
      "default": {"x": 0, "y": 0}
    },
    "scale": {
      "$ref": "/schemas/images/ImageScale.schema.json",
      "default": {"x": 1, "y": 1}
    },
    "images": {
      "type": "array",
      "items": {"$ref": "/schemas/images/Image.schema.json"},
      "default": []
    }
  },
  "additionalProperties": false
}
