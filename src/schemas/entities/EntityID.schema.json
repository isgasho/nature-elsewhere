{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://natureelsewhere.com/schemas/entities/EntityID.schema.json",
  "type": "string",
  "title": "EntityID",
  "description": "Identifier, not necessarily unique, specified at creation time as needed to distinguish among instances of the same type. For example, two entities may both be of type \"bee\" but have \"happy\" and \"angry\" IDs.",
