  "scripts": {
    "start": "webpack-dev-server",
    "test": "npm -s run formatter -- -l && npm -s run build && jest",
    "build": "webpack -p",
    "format": "npm -s run formatter -- --write",
    "formatter": "prettier './**/*.{css,json,html,md,js,ts}'",
    "build:atlas": "aseprite -b assets/images/*.aseprite --filename-format '{tag} {frame}' --list-tags --list-slices --data src/assets/atlas.json --sheet-pack --inner-padding 1 --sheet src/assets/images/atlas.png",
    "palettize": "cd assets && find images -name \\*.aseprite -exec aseprite '{}' -b --palette palette.aseprite --save-as '{}' \\;"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "5.0.0",
    "@types/jest": "24.0.15",
    "@types/webgl2": "0.0.4",
    "@types/webpack-dev-server": "3.1.6",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.3",
    "jest": "24.8.0",
    "pre-commit": "1.2.2",
    "prettier": "1.18.2",
    "raw-loader": "3.0.0",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "typescript": "3.5.2",
    "webpack": "4.35.2",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  }
