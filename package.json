{
  "name": "nature-elsewhere",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/rndmem/nature-elsewhere",
  "repository": "github:rndmem/nature-elsewhere",
  "bugs": "https://github.com/rndmem/nature-elsewhere/issues",
  "license": "GPL-3.0",
  "scripts": {
    "start": "webpack-dev-server -dw",
    "build": "NODE_ENV=production webpack -p",
    "lint": "eslint --cache --max-warnings 0 --report-unused-disable-directives --ext .ts .",
    "style": "prettier './**/*.{md,json}'",
    "format": "run-p -s 'lint -- --fix' 'style -- --write'",
    "test": "run-p -s build lint 'style -- -l' test:\\*",
    "test:unit": "jest",
    "build:atlas": "cd src/assets/textures && aseprite -b *.aseprite --filename-format '{tag} {frame}' --list-tags --list-slices --data atlas.json --sheet-pack --inner-padding 1 --sheet atlas.png",
    "palettize": "cd src/assets/textures && find -name \\*.aseprite -exec aseprite '{}' -b --palette palette.aseprite --save-as '{}' \\;"
  },
  "engines": {
    "node": "9.11.1"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "0.1.2",
    "@types/copy-webpack-plugin": "4.4.1",
    "@types/jest": "22.2.3",
    "@types/node": "9.6.5",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "jest": "22.4.3",
    "npm-run-all": "4.1.2",
    "pre-commit": "1.2.2",
    "prettier": "1.12.1",
    "raw-loader": "0.5.1",
    "ts-jest": "22.4.4",
    "ts-loader": "4.2.0",
    "ts-node": "6.0.0",
    "typescript": "2.8.1",
    "typescript-eslint-parser": "15.0.0",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3"
  }
}
