{
  "name": "nature-elsewhere",
  "author": "niedzielski",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/rndmem/nature-elsewhere",
  "repository": "github:rndmem/nature-elsewhere",
  "bugs": "https://github.com/rndmem/nature-elsewhere/issues",
  "license": "GPL-3.0",
  "scripts": {
    "start": "webpack-dev-server -dw",
    "build": "NODE_ENV=production webpack -p",
    "lint": "prettier './**/*.{json,md,ts,yaml}'",
    "format": "run-p -s 'lint -- --write'",
    "test": "run-p -s build 'lint -- -l' test:\\*",
    "test:unit": "jest",
    "build:textures": "cd src/assets/textures && aseprite -b ../../../assets/*.aseprite --filename-format '{tag} {frame}' --list-tags --list-slices --data atlas.json --sheet-pack --inner-padding 1 --sheet atlas.png",
    "build:text": "js-yaml assets/a-summary-of-you.yaml > src/assets/a-summary-of-you.json",
    "palettize": "cd assets && find -name \\*.aseprite -exec aseprite '{}' -b --palette palette.aseprite --save-as '{}' \\;"
  },
  "engines": {
    "node": "10.7.0"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "0.1.2",
    "@types/copy-webpack-plugin": "4.4.1",
    "@types/jest": "23.3.1",
    "@types/node": "10.5.4",
    "@types/webgl2": "0.0.4",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.2",
    "jest": "23.4.2",
    "js-yaml": "3.12.0",
    "npm-run-all": "4.1.3",
    "pre-commit": "1.2.2",
    "prettier": "1.14.0",
    "raw-loader": "0.5.1",
    "ts-jest": "23.0.1",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "typescript": "2.9.2",
    "webpack": "4.16.3",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  }
}
