  "license": "GPL-3.0",
  "scripts": {
    "start": "webpack-dev-server -dw",
    "test": "npm run -s formatter -- -l && npm run -s build && jest",
    "format": "npm -s run formatter -- --write",
    "build": "webpack -p",
    "formatter": "prettier './**/*.{json,md,ts}'",
    "build:textures": "cd src/assets && aseprite -b ../../assets/*.aseprite --filename-format '{tag} {frame}' --list-tags --list-slices --data atlas.json --sheet-pack --inner-padding 1 --sheet atlas.png",
    "palettize": "cd assets && find -name \\*.aseprite -exec aseprite '{}' -b --palette palette.aseprite --save-as '{}' \\;"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "0.1.2",
    "@types/copy-webpack-plugin": "4.4.2",
    "@types/jest": "23.3.2",
    "@types/webgl2": "0.0.4",
    "@types/webpack-dev-server": "3.1.1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.2",
    "jest": "23.6.0",
    "pre-commit": "1.2.2",
    "prettier": "1.14.3",
    "raw-loader": "0.5.1",
    "ts-jest": "23.10.2",
    "ts-loader": "5.2.1",
    "ts-node": "7.0.1",
    "typescript": "3.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.1",
    "webpack-dev-server": "3.1.9"
  }
}
